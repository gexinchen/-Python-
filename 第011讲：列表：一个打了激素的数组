# 每次从列表的末尾取出一个元素，并将这个元素插入到列表的最前边
list1 = [1, 2, 3, 4, 5, 6]
list1.insert(0, list1.pop())

# 对列表排序
list1.sort()

# remove  删除元素
a = ['内容']
a.remove('内容')

# del  删除列表或其中内容
b = ['内容']
del b[1]

# pop  删除元素，并返回值
c = ['内容']
c.pop(1)

# append  向列表添加内容,一次只能传输一个内容
a = []
a.append('内容')

# append  向列表添加内容,一次只能传输多个内容
b = []
b.extend(['Star', 'T'])

# insert  向列表制定位置添加内容
c = ['A', 'B', 'C']
c.insert(0, 'I')

# 替换列表内容
list1 = [1, [1, 2, ['Star']], 3, 5, 8, 13, 18]
list1[1][2] = ['flower']
