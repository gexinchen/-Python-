#判断字符串中各个字符出现的次数
str1 = ""
str2 = []
for i in range(0,len(str1)):
    if str1[i] == '\n':
        continue
    if str1[i] not in str2:
        str2.append(str1[i])
        i += 1
    if str1[i] in str2:
        i += 1
print (str2)
for l in range(0,len(str2)):
    count = 0
    for a in range(0,len(str1)):
        if str2[l] in str1[a]:
            count += 1
            continue
    print (str2[l],'这个字符总共出现',count,'次')


