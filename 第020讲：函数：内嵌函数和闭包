str2 = str1.replace("\n", "")
b = []
for i in range(3,(len(str2)-4)):
    if (str2[i].islower() == True):
		#判断i的前三位，和后三位是否为大写
        if ((str2[(i-3):i].isupper() == True) and (str2[i+1:i+4].isupper() == True)):
            if ((str2[i-4].isupper() == False) and (str2[i+4].isupper() == False)):
                b.append(str2[i])
print (b)

