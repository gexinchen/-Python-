#输入一个数，并且打印从1到输入的数
temp = input ('请输入一个整数：')
number = int(temp)
num = 1
while number:
	print (num)
	num += 1
	number -= 1


#输入一个数，打印输入数-1的空格，与输入数的*
temp = input ('请输入一个数字：')
number = int(temp)
while number:
	print (' ' * (number-1) + '*' * number)
	number -= 1


#打印99乘法表
for i in range(1,10):
	print ()
	for j in range (1,i+1):
		print (i,'*',j,'=',i*j,end = '  ')
